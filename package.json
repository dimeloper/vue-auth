    "sass-loader": "^7.1.0",
    "stylelint": "^13.2.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.3",
